{
  "name": "trpc-example",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "api:db:up": "cd apps/api && yarn db:up",
    "api:db:down": "cd apps/api && yarn db:down",
    "api:db:logs": "cd apps/api && yarn db:logs",
    "api:db:reset": "cd apps/api && yarn db:reset"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "yarn@4.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
